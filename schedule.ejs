<!DOCTYPE html>
<html>
    <head>
        <link rel="stylesheet" href="app.css">
    </head>
    <body>
        <h2>Results</h2>

        <table>
            <thead>
                <tr>
                  <th>Course Number</th>
                  <th>Course</th>
                  <th>Professor</th>
                  <th>Rating</th>
                  <th>Location and Time</th>
                  <!-- <th>Chilly Pepper</th> -->
                </tr>
            </thead>
            <tbody>
                <% for(var i = 0; i<results.length;i++) {%>
                <tr id="<%=results[i].num%>">
                  <td> <a href="#"><%= results[i].num %></a> </td>
                  <td><%= results[i].name %></td>
                  <td><%= results[i].prof %></td>
                  <td><%= results[i].rating %></td>
                  <td><%= results[i].timeLoc %></td>
                </tr> <% } %>
            </tbody>
        </table>
        <script type="text/javascript" src="animation.js">

    </body>
</html>
